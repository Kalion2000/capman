arch=('x86_64')
url="http://www.alsa-project.org"
license=('LGPL')
groups=()
depends=('glibc')
makedepends=()
optdepends=('python2: for python smixer plugin')
provides=()
conflicts=()
replaces=()
source=("https://dl.bintray.com/chromebrew/chromebrew/alsa_lib-1.1.7-chromeos-x86_64.tar.xz")
sha256sums=('0e5b6e000aedc2ffc9264eb62bf0afb03e06a86a7803c6105489758b14e822f5')

package() {
  install -dm 0755 "$pkgdir/usr"
  cp -a usr/local "$pkgdir/usr"
}
