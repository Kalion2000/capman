pkgname=curl
pkgver=7.64.0
pkgrel=1
pkgdesc="An URL retrieval utility and library"
arch=('x86_64')
url="https://curl.haxx.se"
license=('MIT')
groups=()
depends=('brotli' 'libmetalink' 'libtirpc' 'openldap' 'rtmpdump')
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
options=(!strip staticlibs !zipman !purge)
source=("https://dl.bintray.com/chromebrew/chromebrew/curl-7.64.0-chromeos-x86_64.tar.xz")
sha256sums=('68b0f48be8feefe01582c7fe05ef549d0fd63b581ab5d2821226f5d04c6c3dbe')

package() {
  install -d -m 0755 "$pkgdir/usr"
  cp -a usr/local "$pkgdir/usr"
}
