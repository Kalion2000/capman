pkgname=cvs
pkgver=1.11.23
pkgrel=1
pkgdesc="Concurrent Versions System - a source control system"
arch=('x86_64')
url="http://cvs.nongnu.org/"
license=('GPL')
groups=()
depends=('krb5' 'zlib')
makedepends=()
optdepends=('inetutils: for using cvs over rsh'
            'openssh: for using cvs over ssh')
provides=()
conflicts=()
replaces=()
source=("https://dl.bintray.com/chromebrew/chromebrew/cvs-1.11.23-chromeos-x86_64.tar.xz")
sha256sums=('77bb711a00b0f204092a7833e5f0a45b29d30e1440d0448769637708909fb655')

package() {
  install -dm 0755 "$pkgdir/usr"
  cp -a usr/local "$pkgdir/usr"
}
