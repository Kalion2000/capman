pkgname=dpkg
pkgver=1.19.2
pkgrel=1
pkgdesc="A medium-level package manager for Debian"
arch=('x86_64')
url="https://anonscm.debian.org/git/dpkg/"
license=('custom')
groups=()
depends=('bz2' 'xzutils')
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
source=("https://dl.bintray.com/chromebrew/chromebrew/dpkg-1.19.2-chromeos-x86_64.tar.xz")
sha256sums=('ae5431150d307fc1cbf5635192bd6cb22f82d7fc60d3f7263b169dd4283205dd')

package() {
  install -dm 0755 "$pkgdir/usr"
  cp -a usr/local "$pkgdir/usr"
}
