pkgname=gcc
pkgver=4.9.4
pkgrel=1
pkgdesc="The GNU Compiler Collection - C and C++ frontends"
arch=('x86_64')
url="http://gcc.gnu.org"
license=('custom' 'FDL' 'GPL' 'LGPL')
groups=('base-devel')
depends=('binutils>=2.28' 'gcc-libs=8.2.1+20181127-1' 'libmpc')
makedepends=()
optdepends=('lib32-gcc-libs: for generating code for 32-bit ABI')
provides=('gcc-multilib')
conflicts=()
replaces=('gcc-multilib')
source=("https://dl.bintray.com/chromebrew/chromebrew-cross/gcc-4.9.4-1-chromeos-x86_64.tar.xz")
sha256sums=('1ae1f58ca4b9fee3b0f68856e5d49b2feeb90e259e8d885f339c9aa8091f27c8')

package() {
  install -dm 0755 "$pkgdir/usr"
  cp -a usr/local "$pkgdir/usr"
}
