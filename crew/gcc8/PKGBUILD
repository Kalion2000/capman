license=('custom')
groups=()
depends=('dejagnu' 'gcc7' 'icu4c' 'binutils' 'gmp' 'mpfr' 'mpc' 'isl' 'cloog' 'glibc')
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
options=(!strip staticlibs !zipman !purge)
