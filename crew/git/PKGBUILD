pkgname=git
pkgver=2.20.1
pkgrel=1
pkgdesc="the fast distributed version control system"
arch=('x86_64')
url="https://git-scm.com/"
license=('GPL2')
groups=()
depends=('python27')
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
options=(!strip staticlibs !zipman !purge)
source=("https://dl.bintray.com/chromebrew/chromebrew/git-2.20.1-chromeos-x86_64.tar.xz")
sha256sums=('316842c20bf24202c794eb9c21e46d14bb95069d3667dc5d32e5058bdcdebd89')

package() {
  install -d -m 0755 "$pkgdir/usr"
  cp -a usr/local "$pkgdir/usr"
}
