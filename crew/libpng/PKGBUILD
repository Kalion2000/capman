url="http://www.libpng.org/pub/png/libpng.html"
license=('custom')
groups=()
depends=('sh' 'zlib')
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
source=("https://dl.bintray.com/chromebrew/chromebrew/libpng-1.6.34-chromeos-x86_64.tar.xz")
sha256sums=('172fa194c4a7e185a9bacf7a1932c6dcf71ce0a3d19990fd10a39ee9f6a2171a')

package() {
  install -dm 0755 "$pkgdir/usr"
  cp -a usr/local "$pkgdir/usr"
}
