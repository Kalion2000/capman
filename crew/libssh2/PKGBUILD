pkgrel=1
pkgdesc="A library implementing the SSH2 protocol as defined by Internet Drafts"
arch=('x86_64')
url="https://www.libssh2.org/"
license=('BSD')
groups=()
depends=()
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
options=(!strip staticlibs !zipman !purge)
source=("https://dl.bintray.com/chromebrew/chromebrew/libssh2-1.8.0-chromeos-x86_64.tar.xz")
sha256sums=('6e026450389021c6267a9cc79b8722d15f48e2f8d812d5212501f686b4368e3c')

package() {
  install -d -m 0755 "$pkgdir/usr"
  cp -a usr/local "$pkgdir/usr"
}
