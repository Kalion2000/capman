arch=('x86_64')
url="http://www.xiph.org/vorbis/"
license=('BSD')
groups=()
depends=('libogg')
makedepends=()
optdepends=()
provides=('libvorbisenc.so=2-64' 'libvorbisfile.so=3-64' 'libvorbis.so=0-64')
conflicts=()
replaces=()
source=("https://dl.bintray.com/chromebrew/chromebrew/libvorbis-1.3.5-chromeos-x86_64.tar.xz")
