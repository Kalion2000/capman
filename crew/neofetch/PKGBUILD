pkgname=neofetch
pkgver=3.3.0
pkgrel=1
pkgdesc="A CLI system information tool written in BASH that supports displaying images."
arch=('x86_64')
url="https://github.com/dylanaraps/neofetch"
license=('MIT')
groups=()
depends=()
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
options=(!strip staticlibs !zipman !purge)
source=("https://dl.bintray.com/chromebrew/chromebrew/neofetch-3.3.0-chromeos-x86_64.tar.xz")
sha256sums=('f3ffcb5a6b7c3c5e5f663293db150fd6fde371b65067cc5dc3306a449e8eca75')

package() {
  install -d -m 0755 "$pkgdir/usr"
  cp -a usr/local "$pkgdir/usr"
}
