license=('custom:BSD')
groups=()
depends=('krb5' 'ldns' 'libedit' 'openssl')
makedepends=()
optdepends=('x11-ssh-askpass: input passphrase in X'
            'xorg-xauth: X11 forwarding')
provides=()
conflicts=()
replaces=()
source=("https://dl.bintray.com/chromebrew/chromebrew/openssh-7.6-chromeos-x86_64.tar.xz")
