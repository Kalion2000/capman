arch=('x86_64')
url="http://wput.sourceforge.net/"
license=('custom')
groups=()
depends=('gnutls')
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
source=("https://dl.bintray.com/chromebrew/chromebrew/wput-0.6.2-chromeos-x86_64.tar.xz")
sha256sums=('f2b7ed34899a4d0ea18cc553d0028d16683fcebe0915eed11098911aa663c40b')

package() {
